{
  "block comment": {
    "prefix": "block",
    "body": "\/*\n\t${0}\n*\/"
  },
  "console.log": {
    "prefix": "cl",
    "body": "console.log(\"${1}:\", ${1});${0}"
  },
  "console.error": {
    "prefix": "ce",
    "body": "console.error(\"${1}:\", ${1});${0}"
  },
  "console.warn": {
    "prefix": "cw",
    "body": "console.warn(\"${1}:\", ${1});${0}"
  }
}
