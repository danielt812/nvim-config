{
  "block comment": {
    "prefix": "block",
    "body": "\/*\n\t${0}\n*\/"
  },
  "console.log": {
    "prefix": "cl",
    "body": "console.log(\"${1}:\", ${1});${0}"
  },
  "console.error": {
    "prefix": "ce",
    "body": "console.error(\"${1}:\", ${1});${0}"
  },
  "console.warn": {
    "prefix": "cw",
    "body": "console.warn(\"${1}:\", ${1});${0}"
  },
  "forOf": {
    "prefix": "forof",
    "body": "for (const ${1:element} of ${2:array}) {\n\t${0}\n}"
  },
  "forIn": {
    "prefix": "forin",
    "body": "for (const ${1:property} in ${2:object}) {\n\t${0}\n}"
  },
  "forEach": {
    "prefix": "foreach",
    "body": "${1:iterable}.forEach((${2:item}) => {\n\t${0}\n})"
  },
  "map": {
    "prefix": "map",
    "body": "${1:iterable}.map((${2:item}) => {\n\t${0}\n})"
  },
  "reduce": {
    "prefix": "reduce",
    "body": "${1:iterable}.reduce((${2:accumulator}, ${3:current}) => {\n\t${0}\n}${4:, initial})"
  },
  "filter": {
    "prefix": "filter",
    "body": "${1:iterable}.filter((${2:item}) => {\n\t${0}\n})"
  },
  "find": {
    "prefix": "find",
    "body": "${1:iterable}.find((${2:item}) => {\n\t${0}\n})"
  },
  "every": {
    "prefix": "every",
    "body": "${1:iterable}.every((${2:item}) => {\n\t${0}\n})"
  },
  "some": {
    "prefix": "some",
    "body": "${1:iterable}.some((${2:item}) => {\n\t${0}\n})"
  },
  "try/catch": {
    "prefix": "tc",
    "body": "try {\n\t${0}\n} catch (error) {\n\tthrow error;\n}"
  },
  "try/finally": {
    "prefix": "tf",
    "body": "try {\n\t${0}\n} finally {\n\t\n}"
  },
  "try/catch/finally": {
    "prefix": "tcf",
    "body": "try {\n\t${0}\n} catch (error) {\n\tthrow error;\n} finally {\n\t\n}"
  }
}
